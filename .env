# API Configuration
# Empty string to use package.json proxy (most reliable for CRA)
REACT_APP_API_URL=

# For production build, use:
# REACT_APP_API_URL=http://31.97.70.3:8080/event-manage

# For local backend development, use:
# REACT_APP_API_URL=http://localhost:8080/event-manage

# Disable ESLint for setupProxy.js
DISABLE_ESLINT_PLUGIN=true

# Fix webpack-dev-server allowedHosts error
DANGEROUSLY_DISABLE_HOST_CHECK=true
WDS_SOCKET_PORT=0